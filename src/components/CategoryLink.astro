---
import TaxonomyLink from './core/TaxonomyLink.astro';

interface Props {
    category: string;
}
const { category } = Astro.props;
---

<TaxonomyLink
    class="outline outline-1 font-bold outline-amber-800"
    title={`category: ${category}`}
    href={`/categories/${category}/`}
>{category}</TaxonomyLink>
