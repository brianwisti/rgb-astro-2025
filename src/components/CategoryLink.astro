---
import PCategoryLink from "./core/PCategoryLink.astro";

interface Props {
    category: string;
}
const { category } = Astro.props;
---

<PCategoryLink classNames="category" href={`/categories/${category}/`}
    >{category}</PCategoryLink
>

<style type="text/css">
    a {
        font-weight: bold;
    }
</style>
