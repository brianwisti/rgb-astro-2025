---
import TaxonomyLink from './core/TaxonomyLink.astro';

interface Props {
    category: string;
}
const { category } = Astro.props;
---

<TaxonomyLink
    classNames="category"
    href={`/categories/${category}/`}
>{category}</TaxonomyLink>

<style type="text/css">
    a {
        font-weight: bold;
    }
</style>
